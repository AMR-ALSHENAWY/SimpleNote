<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amr.firebasecreation">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/noteicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/noteicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.amr.simpleNote.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.amr.simpleNote.NoteActivity"/>
        <activity android:name="com.amr.simpleNote.LogInAct">


        </activity>
        <activity android:name="com.amr.simpleNote.SignUpAct"/>
        <activity android:name="com.amr.simpleNote.UserInfo"/>
    </application>

</manifest>